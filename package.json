{
  "name": "react-app",
  "version": "1.1.0",
  "description": "react-app",
  "main": "index.js",
  "author": "Ioan Ungurean",
  "license": "MIT",
  "scripts": {
    "prebuild:dev": "rimraf public",
    "build:dev": "webpack --config devtools/webpack.config",
    "start": "webpack-dev-server --config devtools/webpack.config",
    "prebuild:prod": "rimraf public",
    "build:prod": "webpack -p --config devtools/webpack.config --env.environment=production",
    "preanalysis": "rimraf public | rimraf bundle-analysis-stats.json",
    "analysis": "webpack -p --config devtools/webpack.config --profile --json > bundle-analysis-stats.json --env.environment=production"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-code-splitting": "^1.2.1",
    "react-dom": "^16.1.1",
    "xbem": "^0.0.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template": "^6.1.0",
    "node-sass": "^4.7.2",
    "path": "^0.12.7",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-chunk-hash": "^0.5.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  }
}
